<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="josiane.tradeandgo.app" >

    <uses-permission android:name="com.google.android.glass.permission.DEVELOPMENT" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="josiane.tradeandgo.app.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="josiane.tradeandgo.app.info.Information"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.INFO" />

                <category android:name="android.intent.category.TITRE" />
            </intent-filter>
        </activity>
    </application>

</manifest>
